{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://mb.clean-room.co.kr/$1"
    },
    {
      "source": "/partner/(.*)",
      "destination": "https://pt.clean-room.co.kr/$2"
    },
    {
      "source": "/((?!api|partner).*)",
      "destination": "/index.html"
    }
  ]
}